    files.forEach(file => {
      const serviceMethods = require(\`\${servicesPath}\${file}\`);
      const asyncServiceMethods = convertObjectFunctionsToAsync(serviceMethods);
      replServer.context[\`\${file.split('.')[0]}Service\`] = asyncServiceMethods;
    });
  });
});
"
`;

