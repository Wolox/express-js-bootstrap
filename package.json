{
  "name": "express-js-bootstrap",
  "version": "0.1.0",
  "description": "Kickoff for ExpressJS applications",
  "engines": {
    "node": "6.0.0",
    "npm": "2.14.2"
  },
  "scripts": {
    "cover": "NODE_ENV=testing istanbul cover ./node_modules/mocha/bin/_mocha --compilers js:babel-core/register test/app.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "lint": "./node_modules/eslint/bin/eslint.js \"**/*.js\"",
    "pretest": "npm run lint",
    "front": "./node_modules/.bin/gulp scripts && ./node_modules/.bin/gulp sass && ./node_modules/.bin/gulp watch",
    "start": "./node_modules/.bin/devtool app.js --watch --browser-field",
    "test": "NODE_ENV=testing ./node_modules/mocha/bin/mocha --compilers js:babel-core/register test/app.js"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "main": "app.js",
  "author": "Michel Agopian <michel.agopian@wolox.com.ar>",
  "homepage": "https://github.com/Wolox/express-js-bootstrap",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wolox/express-js-bootstrap.git"
  },
  "bugs": {
    "url": "https://github.com/Wolox/express-js-bootstrap/issues",
    "email": "michel.agopian@wolox.com.ar"
  },
  "dependencies": {
    "angular": "^1.6.0",
    "angular-local-storage": "^0.2.7",
    "angular-translate": "^2.11.1",
    "angular-ui-router": "^0.3.1",
    "body-parser": "^1.14.2",
    "express": "^4.9.4",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^3.0.0",
    "jade": "~1.11.0",
    "jwt-simple": "^0.2.0",
    "morgan": "^1.7.0",
    "node-notifier": "^4.6.1",
    "orm": "^2.1.30",
    "pg": "^4.4.6",
    "request": "^2.76.0",
    "rollbar": "^0.6.2",
    "run-sequence": "^1.2.2"
  },
  "devDependencies": {
    "babel": "6.3.13",
    "babel-core": "6.1.18",
    "babel-eslint": "^6.0.0",
    "babel-preset-es2015": "6.3.13",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "coveralls": "^2.11.9",
    "devtool": "^2.3.0",
    "eslint": "^2.5.1",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0"
  }
}
